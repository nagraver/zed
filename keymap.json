[
    {
        "bindings": {
            "alt-cmd-l": "editor::Format",
            "cmd-d": "editor::DuplicateLineDown",
            "alt-up": "editor::SelectLargerSyntaxNode",
            "alt-down": "editor::SelectSmallerSyntaxNode",
            "alt-shift-up": "editor::MoveLineUp",
            "alt-shift-down": "editor::MoveLineDown",
            "cmd-[": "workspace::ActivateNextPane",
            "cmd-]": "workspace::ActivatePreviousPane",
            "cmd-1": "workspace::ToggleLeftDock",
            "cmd-2": "workspace::ToggleRightDock",
            "cmd-3": "workspace::ToggleBottomDock",
            // "cmd-r": "search::ToggleReplace",
            "cmd-r": "buffer_search::DeployReplace",
            "cmd-4": null,
            "cmd-5": null,
            "cmd-6": null,
            "cmd-7": null,
            "cmd-8": null,
            "cmd-9": null,
            "cmd-0": null,
            "cmd-b": null,
            "cmd-j": null,

            // to reibind or remember
            "cmd-shift-k": "editor::DeleteLine",
            "cmd-l": "editor::SelectLine",
            "cmd-shift-l": "editor::SelectAllMatches",
            "cmd-|": "editor::MoveToEnclosingBracket"
        }
    }
]


